#: background_opacity 0.75

font_size 12.0
# font_family JetBrains Mono NL
# bold_font JetBrains Mono NL Bold
# italic_font JetBrains Mono NL Italic
# bold_italic_font JetBrains NL Bolt Italic

cursor_trail 200
cursor_trail_decay 0.1 0.4
cusror_trail_start_threshold 2
# cursor_trail_color none

# BEGIN_KITTY_THEME
# Adwaita dark
include current-theme.conf
# END_KITTY_THEME


# BEGIN_KITTY_FONTS
font_family      family="Pixel Code"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS